
CREATE TABLE "yeast_strain" (
  "id" INTEGER PRIMARY KEY,
	"name" VARCHAR NOT NULL UNIQUE,
	"owner" VARCHAR NULL,
	"background" VARCHAR NULL,
	"mating_type" VARCHAR NULL,
	"genotype" VARCHAR NULL,
	"genotype_short" VARCHAR NULL,
	"genotype_unique" VARCHAR NULL,
	"freezer_code" VARCHAR NULL,
	"comment" TEXT,
	"is_locked" VARCHAR NULL,
	"created_at" DATETIME NULL,
	"updated_at" DATETIME NULL
);
CREATE INDEX yeast_strain_name ON "yeast_strain" (name);
CREATE INDEX yeast_strain_genotype_unique ON "yeast_strain" (genotype_unique);

.separator "|"
.import "input/yeast_strain.txt" yeast_strain

UPDATE yeast_strain SET owner = NULL WHERE owner = '{NULL}'; 
UPDATE yeast_strain SET background = NULL WHERE background = '{NULL}'; 
UPDATE yeast_strain SET mating_type = NULL WHERE mating_type = '{NULL}'; 
UPDATE yeast_strain SET genotype = NULL WHERE genotype = '{NULL}'; 
UPDATE yeast_strain SET genotype_short = NULL WHERE genotype_short = '{NULL}'; 
UPDATE yeast_strain SET genotype_unique = NULL WHERE genotype_unique = '{NULL}'; 
UPDATE yeast_strain SET freezer_code = NULL WHERE freezer_code = '{NULL}';
UPDATE yeast_strain SET comment = NULL WHERE comment = '{NULL}';  
UPDATE yeast_strain SET is_locked = NULL WHERE is_locked = '{NULL}'; 
UPDATE yeast_strain SET created_at = NULL WHERE created_at = '{NULL}'; 
UPDATE yeast_strain SET updated_at = NULL WHERE updated_at = '{NULL}'; 
VACUUM;

